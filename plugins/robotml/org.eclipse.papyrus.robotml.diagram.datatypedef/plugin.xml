			"3026" = PRIMITIVE_TYPE_CN
			"3017" = ENUMERATION_ENUMERATION_LITERAL_CLN
			"3022" = SIGNAL_CN
			"3028" = COMMENT_CN
			"3029" = CONSTRAINT_CN
			"2016" = SHORT_CUT_DIAGRAM
