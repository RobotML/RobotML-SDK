<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.papyrus.extra.robotml.dynamicValidation.feature"
      label="Papyrus RobotML Dynamic Validation (Incubation)"
      version="1.0.0.qualifier"
      provider-name="Dassault Aviation"
      os="linux,macosx,win32"
      ws="cocoa,gtk,win32"
      nl="en_GB"
      arch="x86,x86_64">

   <description url="https://github.com/RobotML/RobotML-SDK">
      This feature groups the different parts needed to create the
update site allowing the dynamic validation to work. It includes
the DSL for athena, the translation to athena files from the
model, the translation to vle project from athena files.
   </description>

   <copyright url="http://www.example.com/copyright">
