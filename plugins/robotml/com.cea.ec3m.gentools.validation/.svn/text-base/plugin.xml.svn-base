<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.validation.constraintProviders">
      <category
            id="com.cea.ec3m.gentools.validation.constraints"
            name="eC3M rules">
      </category>
      <constraintProvider
            mode="Batch">
         <package
               namespaceUri="http://www.eclipse.org/uml2/3.0.0/UML">
         </package>     
         <constraints
               categories="com.cea.ec3m.gentools.validation.constraints">
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.SystemHasNoPorts"
                  id="com.cea.ec3m.validation.constraints.SystemHasNoPorts_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.SystemHasNoPorts"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  The system component (main instance within deployment plan) may not have ports.
               </description>
               <target
                     class="Package">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.InstanceSpecificationSlotForAllProperties"
                  id="com.cea.ec3m.validation.constraints.InstanceSpecificationSlotForAllProperties_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.InstanceSpecificationSlotForAllProperties"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Is there a slot for all properties of a component within an instance specification?
               </description>
               <target
                     class="InstanceSpecification">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.ConnectorCallMultiplicity"
                  id="com.cea.ec3m.validation.constraints.ConnectorCallMultiplicity_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.ConnectorCallMultiplicity"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Verify that a connection does not originate from a caller with to a server with the server multiplicity > 1
               </description>
               <target
                     class="Property">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.InitialEntryPoints"
                  id="com.cea.ec3m.validation.constraints.InitialEntryPoints_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.InitialEntryPoints"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Verify that at exactly one blocking start routine is defined.
               </description>
               <target
                     class="Package">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.UnconnectedProvPorts"
                  id="com.cea.ec3m.validation.constraints.UnconnectedProvPorts_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.UnconnectedProvPorts"
                  severity="INFO"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  unconnected provided port
               </description>
               <target
                     class="Property">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.UnconnectedReqPorts"
                  id="com.cea.ec3m.validation.constraints.UnconnectedReqPorts_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.UnconnectedReqPorts"
                  severity="INFO"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  unconnected provided port
               </description>
               <target
                     class="Property">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.OperationsAreImplemented"
                  id="com.cea.ec3m.validation.constraints.OperationsAreImplemented_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.OperationsAreImplemented"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Operation is not implemented
               </description>
               <target
                     class="Class">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.PortIsCorrectlyTyped"
                  id="com.cea.ec3m.validation.constraints.PortIsCorrectlyTyped_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.PortIsCorrectlyTyped"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Port is not correctly typed
               </description>
               <target
                     class="Port">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.BehaviorsWithoutSpecification"
                  id="com.cea.ec3m.validation.constraints.BehaviorsWithoutSpecification_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.BehaviorsWithoutSpecification"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Behavior without specification
               </description>
               <target
                     class="OpaqueBehavior">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.ConnectorConnectable"
                  id="com.cea.ec3m.validation.constraints.ConnectorConnectable_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.ConnectorConnectable"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Verify that a connector can be instantiated, i.e. a consistent connector binding can be found.
               </description>
               <target
                     class="Connector">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.AggregationKindValid"
                  id="com.cea.ec3m.validation.constraints.AggregationKindValid_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.AggregationKindValid"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  &quot;eC3M: {0}&quot;
               </message>
               <description>
                  Check whether aggregation kind is consistent with singleton information
               </description>
               <target
                     class="Property">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.ConfigurationAttributeNotSet"
                  id="com.cea.ec3m.validation.constraints.ConfigurationAttributeNotSet_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.ConfigurationAttributeNotSet"
                  severity="WARNING"
                  statusCode="1">
               <message>
                  "eC3M: {0}"
               </message>
               <description>
                  Check whether configuration attributes have a value in the deployment plans
               </description>
               <target
                     class="Slot">
               </target>
            </constraint>
            <constraint
                  class="com.cea.ec3m.gentools.validation.constraints.OperationsWithTwoReturnValues"
                  id="com.cea.ec3m.validation.constraints.OperationsWithTwoReturnValues_ID"
                  lang="Java"
                  mode="Batch"
                  name="com.cea.ec3m.validation.constraints.OperationsWithTwoReturnValues"
                  severity="ERROR"
                  statusCode="1">
               <message>
                  "eC3M: {0}"
               </message>
               <description>
                  Check whether an operation contains more than one return parameter
               </description>
               <target
                     class="Operation">
               </target>
            </constraint>
         </constraints>
      </constraintProvider>
      </extension>
      <extension
         point="org.eclipse.emf.validation.constraintBindings">
   		<clientContext
            default="false"
            id="com.cea.ec3m.validation.context">
         	<selector class="com.cea.ec3m.gentools.validation.ValidationDelegateClientSelector"/>
      	</clientContext>
      	<binding
            context="com.cea.ec3m.validation.context">
           <category
               ref="com.cea.ec3m.gentools.validation.constraints">
           </category>
        </binding>
      </extension>
</plugin>
