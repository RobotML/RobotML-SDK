<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="*.uml"
            id="org.eclipse.proteus.generators.acceleo.athena.ui.popupMenus.contribution.IFile">
         <menu id="org.eclipse.acceleo.module.menu" label="Proteus UML Model" path="additionsAcceleo">
               <groupMarker name="acceleo"/> 
         </menu>
         <action
               class="org.eclipse.proteus.generators.acceleo.athena.ui.popupMenus.AcceleoGenerateAthenaAction"
               enablesFor="+"
               id="org.eclipse.proteus.generators.acceleo.athena.ui.popupMenus.acceleoGenerateAthenaAction"
               icon="icons/default.gif"
               label="Generate to Athena ADL"
               menubarPath="org.eclipse.acceleo.module.menu/acceleo"/>
       	 <action
       	 		class="org.eclipse.proteus.generators.acceleo.athena.ui.popupMenus.AcceleoAutoGenerateAthenaAction"
       	 		enablesFor="+"
       	 		id="org.eclipse.proteus.generators.acceleo.athena.ui.popupMenus.acceleoAutoGenerateAthenaAction"
       	 		icon="icons/default.gif"
       	 		label="Automatic generation to Athena ADL"
       	 		menubarPath="org.eclipse.acceleo.module.menu/acceleo"/>
      </objectContribution>
   </extension>
</plugin>
